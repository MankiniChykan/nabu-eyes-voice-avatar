{
  "name": "nabu-eyes-voice-avatar",
  "version": "0.0.45",
  "description": "Nabu Eyes dashboard custom card for Home Assistant",
  "scripts": {
    "build": "npm run lint && npm run build:dist",
    "build:dist": "node ./tools/prepare-dist.js && rollup -c && node ./tools/copy-assets.js && node ./tools/compress-dist.js",
    "watch": "rollup -c -w",
    "lint": "eslint --ext .ts src",
    "release": "node ./tools/release.js",
    "release:dev": "node ./tools/release.js",
    "test": "node --test"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/MankiniChykan/nabu-eyes-voice-avatar.git"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/lodash": "^4.14.202",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-lit": "^2.1.1",
    "eslint-plugin-prettier": "^5.1.3",
    "lit": "^3.1.0",
    "prettier": "^3.2.5",
    "rollup": "^4.13.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "smob": "^1.5.0",
    "tslib": "^2.6.2",
    "typescript": "^5.4.2"
  },
  "dependencies": {
    "custom-card-helpers": "^1.9.0"
  }
}
